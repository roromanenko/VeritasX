{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CacheOptions": {
    "SizeLimit": 1024,
    "CompactionPercentage": 0.2,
    "DefaultTtlMinutes": 30,
    "IntervalBasedTtl": {
      "ShortTerm": 2,
      "MediumTerm": 10,
      "LongTerm": 30,
      "VeryLongTerm": 60
    }
  },
  "MongoDbOptions": {
    "DatabaseName": "veritasx"
  },
  "ConnectionStrings": {
    "MongoDb": ""
  },
  "DataCollectorOptions": {
    "MaxCandlesPerRequest": 500,
    "RequestsPerMinute": 1000,
    "RetryAttempts": 3,
    "BatchSize": 500,
    "DelayBetweenRequestsMs": 100,
    "MaxConcurrentJobs": 3
  },
  "Jwt": {
   "Issuer": "https://localhost:7265",
   "Audience": "https://localhost:7265",
   "SecretKey": "",
   "ExpirationHours": 24
 }
}
